{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "exhaustive_search",
      "type": "shell",
      "command": "src/exhaustive_search",
      "options": {
        "cwd": "${workspaceFolder}/build"
      },
      "problemMatcher": []
    },
    {
